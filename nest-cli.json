{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "../src/common/assets/**",
        "outDir": "dist/src",
        "watchAssets": true
      },
      {
        "include": "../src/modules/seeder/files/data/**",
        "outDir": "dist/src",
        "watchAssets": true
      }
    ]
  }
}
